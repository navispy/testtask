      <Button id="btnAdd" fx:id="btnAdd" layoutX="35.0" layoutY="362.0" mnemonicParsing="false" onAction="#handleButtonAddPress" prefHeight="24.0" prefWidth="61.0" text="Add" />
      <Button id="btnEdit" fx:id="btnEdit" layoutX="114.0" layoutY="362.0" mnemonicParsing="false" onAction="#handleButtonEditPress" prefHeight="24.0" prefWidth="62.0" text="Edit" />
      <Button id="btnDelete" fx:id="btnDelete" layoutX="194.0" layoutY="362.0" mnemonicParsing="false" onAction="#handleButtonDeletePress" text="Delete" />
   </children>
</AnchorPane>
